package que7;

public class Kohli extends Thread{

	@Override
	public void run() {
		String name=Thread.currentThread().getName();
		int priority=Thread.currentThread().getPriority();
		
		for(int i=0;i<10;i++) {
			System.out.println("Priority of thread "+priority+" ans the thread name is "+name);
		}
	}
	
}
